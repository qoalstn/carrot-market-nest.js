{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/location/(.*)": "<rootDir>/../libs/location/src/$1",
    "@app/location": "<rootDir>/../libs/location/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "y/lib/(.*)": "<rootDir>/../libs/lib/src/$1",
    "y/lib": "<rootDir>/../libs/lib/src"
  }
}